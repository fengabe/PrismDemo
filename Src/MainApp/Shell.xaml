<Window x:Class="MainApp.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://www.codeplex.com/prism" 
        xmlns:inf="clr-namespace:Infrastructure;assembly=Infrastructure"
        Title="Shell" Height="300" Width="300">
    <Grid>
        <DockPanel LastChildFill="True">
            <ContentControl prism:RegionManager.RegionName="{x:Static inf:RegionNames.ToolbarRegion}" DockPanel.Dock="Top" />
            <ContentControl prism:RegionManager.RegionName="{x:Static inf:RegionNames.ContentRegion}" />
        </DockPanel>
    </Grid>
</Window>
